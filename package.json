{
  "name": "mongo-atlas-ip-updater",
  "version": "0.1.0",
  "description": "Update the MongoDB Atlas Network Access list. This is necessary when using dynamic IP addresses.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kailijan/mongo-atlas-ip-updater.git"
  },
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc index.ts --outDir dist",
    "start:prod": "cross-env NODE_ENV=prod node dist/index.js",
    "start:qa": "cross-env NODE_ENV=qs node dist/index.js",
    "start:dev": "cross-env NODE_ENV=dev node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "kailijan",
  "license": "MIT",
  "dependencies": {
    "digest-fetch": "^1.1.6",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.1",
    "public-ip": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.39",
    "cross-env": "^7.0.3",
    "tslint": "^5.20.1",
    "typescript": "^4.2.4"
  }
}
